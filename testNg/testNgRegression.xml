<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="E2E Regression Suite" verbose="1" preserve-order="true">
    <listeners>
        <listener class-name="testComponents.Listeners"/>
    </listeners>
    <!-- pass browser without extra quotes -->
    <groups>
        <run>
            <include name="regression"/>
        </run>
        <!-- pass browser without extra quotes -->
    </groups>
    <parameter name="envUrl" value="https://rahulshettyacademy.com/client"/>
    <parameter name="browser" value="chrome"/>
    <test name="SubmitOrderTests">
        <classes>
            <class name="test.submitOrderTest"/>
            <class name="test.InvalidOrderTest"/>
        </classes>
    </test>
<!--    abc-->
</suite>
